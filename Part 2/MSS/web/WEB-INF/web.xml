<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <context-param>
        <param-name>javax.faces.PROJECT_STAGE</param-name>
        <param-value>Development</param-value>
    </context-param>
    <servlet>
        <servlet-name>MSSServlet</servlet-name>
        <servlet-class>servlets.MSSServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>login</servlet-name>
        <jsp-file>/login.jsp</jsp-file>
    </servlet>
    <servlet>
        <servlet-name>error</servlet-name>
        <jsp-file>/error.jsp</jsp-file>
    </servlet>
    <servlet>
        <servlet-name>profile</servlet-name>
        <jsp-file>/profile.jsp</jsp-file>
    </servlet>
    <servlet>
        <servlet-name>modules</servlet-name>
        <jsp-file>/modules.jsp</jsp-file>
    </servlet>
    <servlet>
        <servlet-name>index</servlet-name>
        <jsp-file>/index.jsp</jsp-file>
    </servlet>
    <servlet>
        <servlet-name>modResults</servlet-name>
        <jsp-file>/modResults.jsp</jsp-file>
    </servlet>
    <servlet>
        <servlet-name>tutorials</servlet-name>
        <jsp-file>/tutorials.jsp</jsp-file>
    </servlet>
    <servlet>
        <servlet-name>timetable</servlet-name>
        <jsp-file>/timetable.jsp</jsp-file>
    </servlet>
    <servlet>
        <servlet-name>appeals</servlet-name>
        <jsp-file>/appeals.jsp</jsp-file>
    </servlet>
    <servlet-mapping>
        <servlet-name>MSSServlet</servlet-name>
        <url-pattern>/MSS/*</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>login</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>error</servlet-name>
        <url-pattern>/error</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>profile</servlet-name>
        <url-pattern>/profile</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>modules</servlet-name>
        <url-pattern>/modules</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>index</servlet-name>
        <url-pattern>/index</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>modResults</servlet-name>
        <url-pattern>/modResults</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>tutorials</servlet-name>
        <url-pattern>/tutorials</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>timetable</servlet-name>
        <url-pattern>/timetable</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>appeals</servlet-name>
        <url-pattern>/appeals</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file>index.jsp</welcome-file>
    </welcome-file-list>
</web-app>
